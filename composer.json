{
  "name": "laravel-notification-channels/ortto",
  "description": "Laravel Notification Channel for Ortto",
  "homepage": "https://github.com/laravel-notification-channels/ortto",
  "license": "MIT",
  "authors": [
    {
      "name": "Simon Hartcher",
      "email": "simon@simonhartcher.com",
      "homepage": "https://simonhartcher.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.2",
    "illuminate/notifications": "~6.0 || ~7.0 || ~8.0 || ~9.0 || ~10.0 || ~11.0",
    "illuminate/support": "~6.0 || ~7.0 || ~8.0 || ~9.0 || ~10.0 || ~11.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "NotificationChannels\\Ortto\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "NotificationChannels\\Ortto\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit",
    "test:coverage": "phpunit --coverage-text --coverage-clover=coverage.clover"
  },
  "config": {
    "sort-packages": true
  }
}
